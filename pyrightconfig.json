{
  "venvPath": "./.venv",
  "venv": ".venv",
  "pythonPath": "./.venv/bin/python",
  "analysis": {
    "autoSearchPaths": true,
    "useLibraryCodeForTypes": true,
    "typeCheckingMode": "off",
    "diagnosticMode": "workspace",
    "logLevel": "error",
    "extraPaths": [
      "./.venv/lib/python3.8/site-packages"
    ]
  }
}

